<blockquote{{ blockWrapperAttr(block, block.data.alignment is defined and block.data.alignment and block.data.alignment != 'left' ? {class: 'text-' ~ block.data.alignment} : {}) }}>
{# for tawildind compression : text-right text-left #}
    <p>{{ block.data.text|raw }}</p>
    <cite>{{ block.data.caption|raw }}</cite>
</blockquote>
